<view class="p20">
    <view class="rechargeBox">
        <view class="label">提现金额<text style="font-size:12px">(手续费：)</text></view>
        <view class="flex_sb line">
            <text class="unit">￥</text>
            <input class="input" type="number" bindinput="getNum"></input>
        </view>
        <view class="yue">钱包余额：{{money}}</view>
    </view>
    <view class="confirm" bind:tap="confirm">提现到微信钱包</view>
</view>


<login-component bind:myevent="listeningEvent" />