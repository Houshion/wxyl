@import '/css/main.wxss';
page{
    background: #fff;
}
.header_box{
    padding: 30rpx 60rpx;
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    z-index: 99;
}
.search_box{
    box-shadow: 0 0 2px #ccc;
    border-radius: 40rpx;
}
.search_box .search{
    width: 20rpx;
    height: 22rpx;
}
.search_box .cancel{
    width: 30rpx;
    height: 30rpx;
}
.content_box{
    padding-top: 140rpx;
}
.list .head_box{
    width: 80rpx;
    height:80rpx;
    border-radius: 50%;
}
.list .head_box image{
    width: 100%;
    height:100%;
    border-radius: 50%;
}
.list .edit_box image{
    width: 24rpx;
    height: 21rpx;
}
.add_box{
    position: fixed;
    left: 50%;
    bottom: 0;
    transform: translateX(-50%);
    z-index: 99;
}
.add_box image{
    width: 134rpx;
    height: 134rpx;
}
.right_box{
    width: 100rpx;
    height: 100%;
    background: #f5f5f5;
}
.right_box>view{
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    flex:1;
}
.right_box .dele{
    width: 26rpx;
    height: 30rpx;
}
