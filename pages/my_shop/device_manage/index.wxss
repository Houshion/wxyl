@import '/css/main.wxss';
page{
    background: #fff;
}
.list .device_box image{
    width: 66rpx;
    height: 66rpx;
}
.right_box{
    width: 200rpx;
    height: 100%;
    background: #f5f5f5;
}
.right_box>view{
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    flex:1;
}
.right_box .check{
    width: 30rpx;
    height: 26rpx;
}
.right_box .dele{
    width: 26rpx;
    height: 30rpx;
}
.feedback{
    color: #f69626;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 99;
}
