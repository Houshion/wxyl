@import '/css/main.wxss';
.add_box{
    position: fixed;
    left: 50%;
    bottom: 0;
    transform: translateX(-50%);
    z-index: 99;
}
.add_box image{
    width: 134rpx;
    height: 134rpx;
}
.right_box{
    width: 100rpx;
    height: 100%;
    background: #f5f5f5;
}
.right_box>view{
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    flex:1;
}
.right_box .dele{
    width: 26rpx;
    height: 30rpx;
}
.check{
    width: 30rpx;
    height: 26rpx;
}
